<?xml version="1.0" encoding="utf-8"?>
<odoo>      
    <record model="ir.module.category" id="module_category_clinic">
        <field name="name">Clinic Managment</field>
        <field name="description">Helps you handle you handle clinic managment.</field>
        <field name="sequence">7</field>
    </record>

     <record id="group_clinic_administrator" model="res.group">
        <field name="name">Administrator</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('group_clinic_receptionist'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>
 
    <record id="group_clinic_receptionist" model="res.group">
        <field name="name">Receptionist</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_clinic_doctor" model="res.group">
        <field name="name">Doctor</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('group_clinic_receptionist'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

     <record id="group_clinic_labarotory_technichian" model="res.group">
        <field name="name">Lab Technichian</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

     <record id="group_clinic_pharmacist" model="res.group">
        <field name="name">Pharmacist</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

     <record id="group_clinic_nurse" model="res.group">
        <field name="name">Nurse</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>