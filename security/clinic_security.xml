<?xml version="1.0" encoding="utf-8"?>
<odoo>      
    <record model="ir.module.category" id="module_category_clinic">
        <field name="name">Clinic Management</field>
        <field name="description">Helps you handle clinic management.</field>
        <field name="sequence">7</field>
    </record>

    <record id="group_clinic_receptionist" model="res.groups">
        <field name="name">Receptionist</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_clinic_administrator" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>


    <record id="group_clinic_doctor" model="res.groups">
        <field name="name">Doctor</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('group_clinic_receptionist'))]"/>
    </record>

    <record id="group_clinic_laboratory_technician" model="res.groups">
        <field name="name">Lab Technician</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_clinic_pharmacist" model="res.groups">
        <field name="name">Pharmacist</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_clinic_nurse" model="res.groups">
        <field name="name">Nurse</field>
        <field name="category_id" ref="module_category_clinic"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>